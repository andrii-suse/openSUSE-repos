<repoindex ttl="0"
    disturl="https://download.opensuse.org"
    distsub="tumbleweed/"
    debugenable="false"
    sourceenable="false">

<repo url="%{disturl}/ports/%{basearch}/%{distsub}/repo/oss"
    alias="repo-oss"
    name="%{alias} (%{distver})"
    enabled="true"
    autorefresh="true"/>

<repo url="%{disturl}/ports/%{basearch}/debug/%{distsub}/repo/oss"
    alias="repo-oss-debug"
    name="%{alias} (%{distver})"
    enabled="true"
    autorefresh="true"/>

<repo url="%{disturl}/ports/%{basearch}/source/%{distsub}/repo/oss"
    alias="repo-oss-source"
    name="%{alias} (%{distver})"
    enabled="true"
    autorefresh="true"/>


</repoindex>
